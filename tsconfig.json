{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": true,
    "skipLibCheck": true,
    "types": [
      "cypress",
      "node"
    ],
    "baseUrl": "."
  },
  "include": [
    "cypress/**/*.ts",
    "cypress/**/*.d.ts",
    "cypress.config.ts"
  ]
}
